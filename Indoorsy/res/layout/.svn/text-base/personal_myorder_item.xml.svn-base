<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <LinearLayout
        style="@style/commonText"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:baselineAligned="false"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/myorderItemImageView"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_gravity="center_vertical"
            android:contentDescription="@string/app_name"
            android:padding="@dimen/homepage_padding_5"
            android:src="@drawable/commodity_firm_product_default" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="fill_parent"
            android:baselineAligned="false"
            android:orientation="vertical"
            android:padding="@dimen/homepage_padding_8" >

            <TextView
                android:id="@+id/myorderItemNameTextView"
                style="@style/commonText"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:textColor="#404040"
                android:textSize="@dimen/homepage_textsize_14" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:baselineAligned="false"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/personal_fragment_myorder_price"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemPriceValueTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/homepage_green_money"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemYuanAndUnitTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    style="@style/commonText"
                    android:layout_marginLeft="15dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/personal_fragment_myorder_number"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemNumberTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/homepage_green_money"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemNumberUnitTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:baselineAligned="false"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/myorderItemAmountsTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/personal_fragment_myorder_amounts"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemAmountsValueTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/homepage_green_money"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/personal_fragment_myorder_yuan"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />
            </LinearLayout>

            <TextView
                android:id="@+id/myorderItemStateTextView"
                style="@style/commonText"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:singleLine="true"
                android:text="@string/personal_fragment_myorder_daifukuan"
                android:textColor="@color/homepage_red_seckill"
                android:textSize="@dimen/homepage_textsize_14"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/myorderItemDaiFuKuanLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:gravity="left"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:id="@+id/myorderItemDaiFuKuanRobotTextView"
                    style="@style/commonText"
                    android:layout_weight="1"
                    android:background="@color/homepage_green_money"
                    android:drawableRight="@drawable/homepage_product_detail_robot"
                    android:gravity="center"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_kefu"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemCancelOrderTextView"
                    style="@style/commonText"
                    android:layout_marginLeft="@dimen/homepage_margin_5"
                    android:layout_marginRight="@dimen/homepage_margin_5"
                    android:layout_weight="1"
                    android:background="#E8A335"
                    android:drawableRight="@drawable/homepage_product_detail_robot_none"
                    android:gravity="center"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_cancel"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemPayTextView"
                    style="@style/commonText"
                    android:layout_weight="1"
                    android:background="#E87435"
                    android:drawableRight="@drawable/homepage_product_detail_robot_none"
                    android:gravity="center"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_pay"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/myorderItemDaiFaHuoLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:gravity="left"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:id="@+id/myorderItemDaiFaHuoRobotTextView"
                    style="@style/commonText"
                    android:layout_weight="1"
                    android:background="@color/homepage_green_money"
                    android:drawableRight="@drawable/homepage_product_detail_robot"
                    android:gravity="center"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_kefu"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemRemindTextView"
                    style="@style/commonText"
                    android:layout_marginLeft="@dimen/homepage_margin_5"
                    android:layout_weight="1"
                    android:background="#E87435"
                    android:gravity="center"
                    android:drawableRight="@drawable/homepage_product_detail_robot_none"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_remind"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemRefundTextView"
                    style="@style/commonText"
                    android:layout_marginLeft="@dimen/homepage_margin_5"
                    android:layout_weight="1"
                    android:background="#EFA22C"
                    android:gravity="center"
                    android:drawableRight="@drawable/homepage_product_detail_robot_none"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_refund"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/myorderItemDaiShouHuoLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:gravity="left"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:id="@+id/myorderItemDaiShouHuoRobotTextView"
                    style="@style/commonText"
                    android:background="@color/homepage_green_money"
                    android:drawableRight="@drawable/homepage_product_detail_robot"
                    android:gravity="center"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_kefu"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemQueRenShouHuoTextView"
                    style="@style/commonText"
                    android:layout_marginLeft="@dimen/homepage_margin_15"
                    android:drawableRight="@drawable/homepage_product_detail_robot_none"
                    android:background="#E87435"
                    android:gravity="center"
                    android:padding="@dimen/homepage_padding_5"
                    android:text="@string/personal_fragment_myorder_confirm"
                    android:textColor="@android:color/white"
                    android:textSize="@dimen/homepage_textsize_14" />
            </LinearLayout>

            <TextView
                android:id="@+id/myorderItemEvaluationTextView"
                style="@style/commonText"
                android:layout_width="100dp"
                android:background="#E8A335"
                android:gravity="center"
                android:padding="@dimen/homepage_padding_5"
                android:paddingLeft="25dp"
                android:paddingRight="25dp"
                android:text="@string/personal_fragment_myorder_pingjia"
                android:textColor="@android:color/white"
                android:drawableRight="@drawable/homepage_product_detail_robot_none"
                android:textSize="@dimen/homepage_textsize_14"
                android:visibility="gone" />

            <LinearLayout
                android:id="@+id/myorderItemRefundLinearLayout"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:baselineAligned="false"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/personal_fragment_myorder_refund"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    android:id="@+id/myorderItemRefundValueTextView"
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@color/homepage_green_money"
                    android:textSize="@dimen/homepage_textsize_14" />

                <TextView
                    style="@style/commonText"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="@string/personal_fragment_myorder_yuan"
                    android:textColor="@color/homepage_gray_text"
                    android:textSize="@dimen/homepage_textsize_14" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>