package com.indoorsy.frash.homepage.data.bean;

import java.io.Serializable;
import java.util.List;

@SuppressWarnings("serial")
public class SeckKill implements Serializable {

	private List<SeckTime> secktime;// 秒杀3个时间集合
	private List<SeckTk> listk2;// 已结束
	private List<SeckTk> listk; // 正在进行中
	private List<SeckTk> listk3;// 即将开始

	/**
	 * 获取 秒杀时间集合
	 */
	public List<SeckTime> getSecktime() {
		return secktime;
	}

	public void setSecktime(List<SeckTime> secktime) {
		this.secktime = secktime;
	}

	/**
	 * 获取 当前秒杀内容集合
	 */
	public List<SeckTk> getListk() {
		return listk;
	}

	public void setListk(List<SeckTk> listk) {
		this.listk = listk;
	}

	public List<SeckTk> getListk2() {
		return listk2;
	}

	public void setListk2(List<SeckTk> listk2) {
		this.listk2 = listk2;
	}

	public List<SeckTk> getListk3() {
		return listk3;
	}

	public void setListk3(List<SeckTk> listk3) {
		this.listk3 = listk3;
	}
	
}
