package com.indoorsy.frash.homepage.data.bean;

import java.io.Serializable;
import java.util.List;

public class ProductDetail implements Serializable {

	private static final long serialVersionUID = 1L;

	
	private List<DetailListpu> Listpu;// 商品单位价格集合
	private List<DetailLictr> lictr;// 效果图集合
	private List<DetailComment> listev;// 评价的集合
	private List<DetailLictdl> lictdl;// 配送的集合
	private List<DetailListcd> listcd;// 产品介绍的集合

	private int repertory;// 库存
	private String name;// 产品名称
	private int comment;// 评论数量
	private boolean collectgoods;//是否收藏
	private String collectId;//收藏id
	 

	/**
	 * 获取 商品单位价格集合
	 * 
	 * @return
	 */
	public List<DetailListpu> getListpu() {
		return Listpu;
	}

	public void setListpu(List<DetailListpu> listpu) {
		Listpu = listpu;
	}

	/**
	 * 获取 评价的集合
	 * 
	 * @return
	 */
	public List<DetailComment> getListev() {
		return listev;
	}

	public void setListev(List<DetailComment> listev) {
		this.listev = listev;
	}

	/**
	 * 获取 配送的集合
	 * 
	 * @return
	 */

	public List<DetailLictdl> getLictdl() {
		return lictdl;
	}

	public void setLictdl(List<DetailLictdl> lictdl) {
		this.lictdl = lictdl;
	}

	/**
	 * 获取 产品介绍的集合
	 * 
	 * @return
	 */
	public List<DetailListcd> getListcd() {
		return listcd;
	}

	public void setListcd(List<DetailListcd> listcd) {
		this.listcd = listcd;
	}

	/**
	 * 获取 库存
	 * 
	 * @return
	 */
	public int getRepertory() {
		return repertory;
	}

	public void setRepertory(int repertory) {
		this.repertory = repertory;
	}

	/**
	 * 获取 产品名称
	 * 
	 * @return
	 */
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	/**
	 * 获取 评论数量
	 * 
	 * @return
	 */
	public int getComment() {
		return comment;
	}

	public void setLictr(List<DetailLictr> lictr) {
		this.lictr = lictr;
	}

	/**
	 * 获取 效果图集合
	 * 
	 * @return
	 */

	public List<DetailLictr> getLictr() {
		return lictr;
	}

	public void setComment(int comment) {
		this.comment = comment;
	}

	/**
	 * 获取 商品是否收藏
	 * 
	 * @return
	 */
	public boolean isCollectgoods() {
		return collectgoods;
	}

	public void setCollectgoods(boolean collectgoods) {
		this.collectgoods = collectgoods;
	}

	/**
	 * 获取 收藏商品id
	 * 
	 * @return
	 */
	public String getCollectId() {
		return collectId;
	}

	public void setCollectId(String collectId) {
		this.collectId = collectId;
	}
}
